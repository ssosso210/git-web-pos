<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dobbypos.model.mapper.StoreMapper"><!-- 클래스 또는 인터페이스 역할 -->
	
	<select id="selectStoreNameListById" parameterType="String" resultType="String">
		SELECT storename
		FROM store
		WHERE storename LIKE concat(#{ id }, '%')
	</select>
	
	<select id="selectStoreCodeListByStoreCode" parameterType="String" resultType="String">
		SELECT storecode
		FROM store
		WHERE storecode LIKE concat(#{ id }, '%')
	</select>
	
	<insert id="insertStore" parameterType="Store">
		INSERT INTO store
		VALUES (#{storeCode}, #{address}, #{storeName}, #{managerName}, #{email}, #{phoneNo}, #{hqCode})
	</insert>
		
</mapper>






